<!--<AnchorPane minHeight="540.0" minWidth="960.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.181"> -->

<GridPane minHeight="540.0" minWidth="960.0" xmlns="http://javafx.com/javafx/10" xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
	  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.3" prefWidth="100.0" />
	  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.3" prefWidth="100.0" />
	    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.3" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
	  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="70.0" valignment="CENTER" vgrow="SOMETIMES" />
	    <RowConstraints maxHeight="1.7976931348623157E308" minHeight="200.0" valignment="TOP" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
	   <Label text="Téléservice" textAlignment="CENTER" GridPane.columnIndex="1">
	      <font>
	         <Font name="System Bold" size="48.0" />
	      </font>
	   </Label>
	   <ImageView fitHeight="60.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
	      <image>
	         <Image url="@../../img/logo.png" />
	      </image>
	   </ImageView>
	   <HBox fx:id="mainBoxLoged" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
	      <children>
	         <Label fx:id="mainLabelName" text="Label" />
	         <Button fx:id="mainBtnLogout" alignment="CENTER" mnemonicParsing="false" text="Déconexion">
	            <HBox.margin>
	               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	            </HBox.margin>
	         </Button>
	      </children>
	   </HBox>
	   <Separator prefWidth="200.0" GridPane.columnSpan="3" GridPane.valignment="BOTTOM" />
	   <VBox fx:id="mainContainer" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnSpan="3" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP" />
	</children>
</GridPane>
