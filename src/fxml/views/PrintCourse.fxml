                  <Label text="Code postal" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <Label text="Attente :" GridPane.halignment="RIGHT" />
                  <Label fx:id="affHeureRetour" text="xxHxx" GridPane.rowIndex="2" />
                  <Label fx:id="affAdresseRetour" text="xxxx xx xxx, xx" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="affLocaliteRetour" text="xxxxxxx" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <Label fx:id="affCpRetour" text="xxxx" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                  <Label fx:id="affAttente" text="oui/non" GridPane.columnIndex="1" GridPane.halignment="LEFT" />
