<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox maxHeight="-Infinity" minHeight="63.0" minWidth="100.0" style="-fx-border-color: black;">
         <children>
            <HBox>
               <children>
                  <Label fx:id="heureStart" text="xxHxx" />
                  <Label text=" - " />
                  <Label fx:id="heureEnd" text="xxHxx" />
               </children>
            </HBox>
            <Label fx:id="titre" text="Titre" />
            <Label fx:id="description" text="description" wrapText="true" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
